{
    "version": 2,
    "builds": [
      {
        "src": "src/app.ts",
        "use": "@vercel/node"
      },
      {
        "src": "src/route.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/route",
        "dest": "src/route.ts"
      },
      {
        "src": "/",
        "dest": "src/app.ts"
      }
    ]
  }